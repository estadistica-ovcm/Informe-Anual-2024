rowwise() %>%
mutate(ymid = ymax - (ymax - ymin)/2) %>%
ungroup() %>%
mutate(Label = paste0("<span style='font-size:15pt'>**",
formatC(round(Porcentaje,1), big.mark=".", decimal.mark=","),
"%**</span><br><span style='font-size:10pt'>",
formatC(Cantidad, big.mark = ".", decimal.mark = ","),
"</span>"))
